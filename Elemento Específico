public class HelloWorld{

     public static void main(String []args){
        int [] num = {1,2,3,4,5};
         System.out.println("El arreglo contiene " + num.length + " elementos. ¿Qué elemento desea conocer?");
        
        int cual = Integer.parseInt(System.console().readLine());
       
          System.out.println("El elemento " + cual + " es: " + num[cual + 1] );
     }
}
